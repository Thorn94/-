  height: auto;
  background-color: #fff;
}
.center {
  width: 90%;
  margin: 0 auto;
}
.section {
  width: 100%;
  height: auto;
  padding: 25px 0 15px 0;
  color: black;
  font-size: 16px;
  border-bottom: 1px solid #f6f6f6;
}
.bank,.bank1{
  font-size: 16px;
  /* display: inline-flex; */
  position: relative;
}

.num {
  text-align: right;
  color: darkgray;
  position: absolute;
  top: 0;
  right: 0;
}
.num1{
  text-align: right;
  color: red;
  position: absolute;
  top: 0;
  right: 0;
}
.submit {
  margin-top: 30px;
}
.btn {
  width: 100%;
  height: 45px;
  background-color: #26a2ff;
  color: #fff;
  text-align: center;
  line-height: 45px;
  border-radius: 25px;
  font-size: 18px;
  border: 0;
  outline: none;
}